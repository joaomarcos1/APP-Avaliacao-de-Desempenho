<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#606060"
    tools:context="edu.umich.PowerTutor.ui.resultados_teste_01_app">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Aplicativo "
        android:textSize="18pt"
        android:layout_marginTop="0dp"
        android:id="@+id/txt_Nome_APP_TESTE_01_APP"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Média:"
        android:id="@+id/textView8"
        android:layout_below="@+id/txt_Nome_APP_TESTE_01_APP"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Variância:"
        android:id="@+id/textView12"
        android:textSize="10pt"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Desvio Padrão:"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/txt_media_teste_01_APP"
        android:textSize="08pt"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignLeft="@+id/variancia_teste_01_APP" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/variancia_teste_01_APP"
        android:textSize="08pt"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignLeft="@+id/txt_Desvio_Padrao_Teste_01_APP" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/txt_Desvio_Padrao_Teste_01_APP"
        android:textSize="08pt"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignParentRight="true" />


    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/chart_teste_01_app"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_below="@+id/textView13"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_above="@+id/btn_voltar_teste_01_app" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_blue"
        android:text="@string/voltar_tela_teste_01app"
        android:id="@+id/btn_voltar_teste_01_app"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />
<!--
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salvar Gráfico"
        android:id="@+id/btn_Salvamento_Grafico"
        android:layout_above="@+id/btn_Salvamento_Relatorio"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Salvar Relatório"
        android:id="@+id/btn_Salvamento_Relatorio"
        android:layout_above="@+id/btn_voltar_teste_01_app"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true" />-->

</RelativeLayout>
